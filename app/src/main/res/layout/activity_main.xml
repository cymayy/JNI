<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:onClick="add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="javaCallc-for_add" />

    <Button
        android:onClick="dealStr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="javaCallc-for_dealStr" />
    <Button
        android:onClick="increaseArrayEles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="javaCallc-for_increaseArrayEles" />
    <Button
        android:onClick="checkPwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="javaCallc-for_checkPwd" />
</LinearLayout>